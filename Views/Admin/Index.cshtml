@model IEnumerable<Ataa.Models.RequestForm>

@{
    Layout = "~/Views/Shared/_Layout1.cshtml";
    ViewData["Title"] = "Dashboard";

}

<div class="row">
    <div class="col-md-4 stretch-card grid-margin ">
        <div class="card bg-gradient-danger card-img-holder text-white">
            <div class="card-body">
                <img src="~/assets1/images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                <h4 class="font-weight-normal mb-3">
                    Users <i class="mdi mdi-human-greeting mdi-24px float-right"></i>
                </h4>
                <h2 class="mb-5">@ViewData["Users"]</h2>
                <h6 class="card-text">Including Admins</h6>
            </div>
        </div>
    </div>
    <div class="col-md-4 stretch-card grid-margin">
        <div class="card bg-gradient-info card-img-holder text-white">
            <div class="card-body">
                <img src="~/assets1/images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                <h4 class="font-weight-normal mb-3">
                    Services Requests <i class="mdi mdi-clipboard-text mdi-24px float-right"></i>
                </h4>
                <h2 class="mb-5">@ViewData["Requests"]</h2>
                <h6 class="card-text">Including Rejected</h6>
            </div>
        </div>
    </div>
    <div class="col-md-4 stretch-card grid-margin">
        <div class="card bg-gradient-success card-img-holder text-white">
            <div class="card-body">
                <img src="~/assets1/images/dashboard/circle.svg" class="card-img-absolute" alt="circle-image" />
                <h4 class="font-weight-normal mb-3">
                    Donated Time <i class="mdi mdi-clock mdi-24px float-right"></i>
                </h4>
                <h2 class="mb-5">@ViewBag.donatedTime</h2>
                <h6 class="card-text">By Hours</h6>
            </div>
        </div>
    </div>
</div>
<div style="margin:0 auto" class="row">
    <div style="margin:0 auto" class="row pl-0 pr-0">
